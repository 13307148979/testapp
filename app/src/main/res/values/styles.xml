<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!-- 一些控件的风格 -->
    <style name="accountEditText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">#EEEDF3</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/text66</item>
        <item name="android:textSize"> @dimen/top_title_size</item>
    </style>
    <style name="arrowRightGreenImageView">
        <item name="android:layout_width">@dimen/margin_choose_06</item>
        <item name="android:layout_height">@dimen/margin_choose_12</item>
        <item name="android:background">@drawable/arrow_right_green</item>
        <item name="android:layout_marginLeft">8dp</item>
    </style>
    <style name="paddinglr16Andtb14">
        <item name="android:paddingRight">@dimen/margin_choose_16</item>
        <item name="android:paddingLeft">@dimen/margin_choose_16</item>
        <item name="android:paddingTop">@dimen/margin_choose_14</item>
        <item name="android:paddingBottom">@dimen/margin_choose_14</item>
    </style>

    <style name="chooseOrEditLinearLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/white</item>
        <item name="android:paddingTop">@dimen/margin_choose_14</item>
        <item name="android:paddingBottom">@dimen/margin_choose_14</item>
        <item name="android:paddingRight">@dimen/margin_choose_16</item>
        <item name="android:paddingLeft">@dimen/margin_choose_16</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="pop_dialog_choose" parent="@android:style/Theme.Holo.Light.Dialog">
        <item name="android:windowBackground">@drawable/btn_white_shape_trans</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:dividerHeight">1.0px</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.6</item>
    </style>

    <style name="pop_dialog_choose3" parent="@android:style/Theme.Holo.Light.Dialog">
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:dividerHeight">1.0px</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.6</item>
    </style>
    <style name="pop_dialog_choose4" parent="@android:style/Theme.Black.NoTitleBar.Fullscreen">
        <item name="android:windowBackground">@color/subtransparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:backgroundDimAmount">0.8</item>
    </style>
    <style name="pop_dialog_choose5" parent="@android:style/Theme.Black.NoTitleBar.Fullscreen">
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimAmount">0.8</item>
    </style>
    <style name="pop_dialog_choose2" parent="@android:style/Theme.Holo.Light.Dialog">
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>

        <item name="android:windowDisablePreview">true</item>
        <item name="android:windowFullscreen">true</item>

        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:dividerHeight">1.0px</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.6</item>
    </style>
    <!--  一些线的风格 -->
    <style name="pop_height_line">
        <item name="android:layout_width">0.5dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/main_backcolor</item>
    </style>
    <style name="width_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/text99</item>
    </style>
    <style name="width_line_d3">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/textd3</item>
    </style>
    <style name="width_line4">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:background">@color/main_backcolor</item>
    </style>
    <style name="width_line3">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/text64</item>
    </style>
    <style name="width_line2">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:background">@color/main_backcolor</item>
    </style>
    <style name="main_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/main_line</item>
    </style>
    <style name="pop_width_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dip</item>
        <item name="android:background">@color/list_divider</item>
    </style>

    <style name="form_explain_text">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@color/text32</item>
    </style>

    <style name="table_content_tv">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/text64</item>
        <item name="android:textSize">16.0sp</item>
        <item name="android:ellipsize">end</item>
    </style>
    <!-- ActionSheet进出动画 -->
    <style name="DialogAnimation" parent="@android:style/Animation.Dialog">
        <item name="android:windowEnterAnimation">@anim/slide_in_bottom</item>
        <item name="android:windowExitAnimation">@anim/slide_out_bottom</item>
    </style>

    <style name="press_more_ll2">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:clickable">true</item>
        <item name="android:paddingLeft">30dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/btn_map_choose_selector3</item>
        <item name="android:onClick">onClick</item>
    </style>

    <style name="text_editStyle" >
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_width">40dp</item>
        <item name="android:background">@drawable/phone_yanzhengma_textview_1</item>
        <item name="android:gravity">center</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/blue</item>
        <item name="android:textSize">22sp</item>
    </style>
</resources>
